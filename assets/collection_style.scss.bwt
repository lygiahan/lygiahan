/*============================================================================
13. FILTER
==============================================================================*/

.filter-group {
	padding: 10px 0px 0px 0px;
	ul {
		max-height: 235px;
		overflow-y: auto;
		&.size_xx {
			overflow-y: inherit !important;
		}
		&::-webkit-scrollbar-track {
			-webkit-box-shadow: inset 0 0 4px rgba(0, 0, 0, 0.3);
			-moz-box-shadow: inset 0 0 4px rgba(0, 0, 0, 0.3);
			border-radius: 10px;
			background-color: #F5F5F5;
		}
		&::-webkit-scrollbar {
			width: 8px;
			background-color: #F5F5F5;
		}
		&::-webkit-scrollbar-thumb {
			border-radius: 10px;
			-webkit-box-shadow: inset 0 0 2px rgba(0, 0, 0, 0.3);
			-moz-box-shadow: inset 0 0 2px rgba(0, 0, 0, 0.3);
			background-color: $main-color;
		}
	}
}

.filter-group-title {
	display: block;
	font-size: em(18px);
	margin-bottom: 10px;
}

.filter-group {
	position:relative;
}

.filter-item {
	margin: 0;
	cursor: pointer;
	line-height: 35px;
	min-width: 100%;
	float: left;
	position:relative;
	padding-right: 8px;
	@media (max-width:991px){
		padding-right: 0px !important;
	}
	.label_relative {
		position:relative;
		.fa {
			&:before {
			}
		}
		.filter_tt {
			padding-left: 5px;
			display: inline-block;
		}
	}
	&.filter-color-size {
		min-width: 0px;
		padding:0px;
	}
	&.filter-size {
		min-width: 0px;
		padding: 0px;
		background: #fff;
		text-align: center;
		margin-right: 5px;
		margin-bottom: 5px;
		line-height: 25px;
		color:#252525;
		position: relative;
		.fa {
			opacity:0;
			width: 30px;
			height:30px;
			position:relative;
		}
		.tags_size {
			color: $text-color;
		}
	}
	.name_color {
		line-height: 25px;
		display: inline-block;
		height: 20px;
		position: absolute;
		font-size: 14px;
		color: lighten($text-color,30%);
		text-transform: inherit;
	}
	* {
		cursor: pointer;
		color: $text-color;
		font-family: $font-base;
		font-size: 14px;
		font-weight: 400;
		margin:0;
		@media (min-width:992px)and(max-width:1199px){
			font-size: 12px;
		}
		&:active {
			color: $main-color;
		}
	}
	&.size {
		min-width:unset;
		width:40px;
		height:40px;
		line-height:40px;
		line-height:40px;
		border: 1px solid #ebebeb;
		margin-right: 10px;
		margin-bottom:10px;
		background: #fff;
		position: relative;
		span {
			width:40px;
			height:40px;
			line-height:40px;
			line-height:40px;
		}
		.tags_size {
			width: 40px;
			height: 40px;
			font-size: 13px;
			display: inherit;
			position:relative;
			position:absolute;
			input {
				width:40px;
				height:40px;
				&:checked+span {
					color:$hover;
				}
			}
			.fa {
				opacity:0;
			}
			&:before {
				display:block;
				content: "";
				width: 40px;
				height: 40px;
				position: absolute;
				left: 0px;
				top: 0;
				border-radius: 3px;
				opacity:0;
			}
			span {
				position: absolute;
				top: 0;
				left: 0;
				width: 40px;
				height: 40px;
				margin: 0;
				text-align: center;
				font-family:$font-base;
				color:$text-color;
				&:after {
					opacity:0;
				}
			}
			&:hover {
				span {
					color:$hover;
				}
			}


		}

		input:checked+.tags_size:before{
			opacity:0;
		}

	}
	&.color{
		min-width: 20px;
		a,label{
			width: 100%; height: 100%;
			display: inline-block;
			font-size: 0;
		}
		.fa{
			width: 20px;
			height: 20px;
			background: red;
			border-radius: 50%;
			position: relative;
			&:before{
				display: none;
			}
			&.trang{
				&:hover:before{
					display: block;
					content: "";
					width: 26px;
					height: 26px;
					position: absolute;
					left: -4px;
					top: -4px;
					border: 1px solid #e2e2e2;
					border-radius: 50%;
				}
			}
			&:hover:before{
				display: block;
				content: "";
				width: 26px;
				height: 26px;
				position: absolute;
				left: -3px;
				top: -3px;
				border: 1px solid #e2e2e2;
				border-radius: 50%;
			}
		}
		input:checked+.fa:before{
			display: block;
			content: "";
			width: 26px;
			height: 26px;
			position: absolute;
			left: -3px;
			top: -3px;
			border: 1px solid #e2e2e2;
			border-radius: 50%;
		}
		input:checked+.fa.trang:before{
			display: block;
			content: "";
			width: 26px;
			height: 26px;
			position: absolute;
			left: -4px;
			top: -4px;
			border: 1px solid #e2e2e2;
			border-radius: 50%;
		}
	}
}

.color input:checked+.fa:after {
	display: none;
}

.filter-item--green *:hover {
	color: $hover;
}

.filter-item--half {
	display: inline-block;
	width: 48%;
	white-space: nowrap;
	overflow: hidden;
	margin-right: 3px;
}

.filter-item--check-box {
	input {
		display: block;
		z-index:9;
		width: 20px;
		height: 20px;
	}
	.fa {
		margin-right: 5px;
		width: 14px;
	}
	input {
		+ .fa:before {
			content: "\f096";
			font-family:'FontAwesome';
		}
		&:checked + span {
			color: $hover;
		}

		&:checked + .fa, + span {
			color: $main-color;
			&:before {
				border-color:$main-color;
			}
			&:after {
				font-family:'FontAwesome';
				content: "\f14a";
				color: $main-color;
				position: absolute;
				top: 10px;
				font-size: 14px;
				left: 0px;
			}
		}
	}
}

.showstt{
	padding: 0 20px;
	height: 40px;
	border: 1px solid #ebebeb;
	line-height: 40px;
	border-radius: 20px;
	margin-bottom: 20px;
	background-position: right center;
	background-repeat: no-repeat;
	background-position: right 20px center;
	padding-right: 28px;
	text-indent: .01px;
	cursor: pointer;
	background-image: url({{'ico-select.png'|asset_url}})!important;
}
.dq-filterxx{
	.active{
		ul{
			display:block !important;
		}
	}
	.filter-group ul {
		max-height: 265px;
		overflow-y: auto;
		border: 1px solid #ebebeb;
		padding: 20px;

	}
}

.filter-container__selected-filter {
	margin-bottom: 10px;
	padding-bottom: 5px;
	border-bottom: 0;
}

.filter-container__selected-filter-header {
	margin-bottom: 5px;
}

.filter-container__selected-filter-header-title {
	font-size: 18px;
