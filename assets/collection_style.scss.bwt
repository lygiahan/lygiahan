/*============================================================================
#Variable
==============================================================================*/
$gutter: 30px;
$main-color: {{ settings.main-color }};
$font-base:  "Roboto",sans-serif;
$hover: {{ settings.hover }};
$text-color: {{ settings.color_body_text }};
$price:{{ settings.color_price }};



.collection-title{
	color:$text-color;
	padding-right:200px;
	padding-bottom:10px;
	font-family: $font-base;
	font-size: 18px;
	font-weight: 700;
	margin: 0px;
	border:0;
	text-transform: unset;
	line-height: 41px;
	@media(max-width:767px){
		padding-right:0;
	}
}
.search-main {
	.products-view-grid {
		border-top: none;
		display: block;
		float: left;
		width: 100%;
	}
	.title-head {
		font-family: $font-base;
		text-transform: none;
		font-size:20px;
		font-weight:700;
		color:$text-color;
		@media (max-width:991px) {
			font-size:16px;
		}
	}
	.form_search_h {
		@media (min-width:768px){
			width: 300px;
			float: left;
			line-height: 2.1;
			border-radius: 0;
			border-top-left-radius: 3px;
			border-bottom-left-radius: 3px;
		}
	}
	.btn_search_h {
		@media (min-width:768px){
			border-radius: 0;
			margin-left: -1px;
			border-top-right-radius: 3px;
			border-bottom-right-radius: 3px;
		}
	}
	.cls_search {
		.item_product_main .product-thumbnail .image_thumb {
			@media (min-width: 1200px) {
				height:262.5px;
			}
			img{

				@media (min-width: 1200px) {
					min-height:262.5px;
				}
				&.loaded {
					min-height:unset;
					height:unset;
				}
			}
		}
	}
}
.sort-cate {
	display:inline-block;
	float:right;
	@media(max-width:767px){
		float:left;
	}
}

/*STYLE SẮP XẾP NEW*/
.sort-cate{
	.sort-cate-left{
		float: left;
		margin-top: 5px;
		h3{
			font-size: 14px;
			font-weight: 700;
			float: left;
			margin: 0;
			margin-top: 2px;
			color: $text-color;
			display:inline-block;
			float:left;
			width: 100px;
		}
		ul {
			float:left;
			display: block;
			padding-left:0;
			width:calc(100% - 100px);
			-webkit-width:calc(100% - 100px);
			-o-width:calc(100% - 100px);
			-os-width:calc(100% - 100px);
			-moz-width:calc(100% - 100px);
			li {
				float: left;
				padding: 0 0 0 20px;
				@media (min-width: 768px) and (max-width: 991px){
					&:first-child {
						padding-left: 0px;
					}
				}	
				&.active {
					a {
						color: $hover;
					}
				}
				a {
					font-size: 14px;
					position: relative;
					i {
						height: 14px;
						width: 14px;
						border: 1px solid #cdcfdd;
						border-radius: 3px;
						display: inline-block;
						vertical-align: middle;
						border-radius:14px;
						margin: -3px 5px 0 0; 
						position: relative;
					}
				}
				&:hover, &.active{
					i{
						background: $hover;
						height: 14px;
						width: 14px;
						border: none;
						border-radius:14px;
						&:before {
							font-family: 'Font Awesome\ 5 Free';
							font-weight:900;
							content: "\f00c";
							color: #fff;
							position: absolute;
							width: 14px;
							border-radius:14px;
							height: 14px;
							line-height: 14px;
							top: 0px;
							font-size: 10px;
							left: 1px;
						}
					}
				}
			}
		}
	}
	.sort-cate-right{
		float: right;
		h3{
			font-size: 14px;
			font-weight: normal;
			float: left;
			margin: 0;
			margin-top: 5px;
			margin-right: 5px;
		}
		.view-mode{
			display:inline-block;
			@media(max-width: 480px){
				text-align:center;
				display:block;
			}
			a{
				border: 1px solid #eaeced;
				width: 65px;
				display: inline-block;
				color: #333;
				font-size: 14px;
				height: auto;
				line-height: inherit;
				float: left;
				border-radius: 0;
				height: 30px;
				line-height: 30px;
				@media(min-width: 413px) and (max-width: 415px){
					width: 90px;
				}
				@media(min-width: 374px) and (max-width: 380px){
					width: 80px;
				}
				&.active, &:hover{
					color: $hover;
				}
				&+a{
					margin-left: 5px;
				}
				@media(max-width: 480px){
					float:none;
				}
			}
		}
		@media(max-width: 480px){
			width: 50%;
			h3{
				display:block;
				text-align:left;
				margin-right: 0;
				float: none;
			}
		}
	}
	@media(max-width: 480px){
		.sort-cate-right-mobile{
			width: 100%;
			float: left;
			#sort-by{
				float:none;
				margin-left: 0px;
				width: 100%;
				padding: 5px 0;
				.left{
					display:block;
					float:left;
					line-height: 1.4;
					font-weight: normal;
					margin-right: 10px;
					margin-top: 5px;
					width: 60px;
				}
				ul{
					width: calc(100% - 80px);
					&>li{
						border: 1px solid #eaeced;
						padding: 0 5px;
						width: 100%;
						line-height: 28px;
					}
					ul{
						background-color: #fff;
						width: 100% !important;
						top: 27px;
						li{
							width: 100%;
							border:none;
						}
					}
				}
			}
		}
	}
}
.dqdt-sidebar {
	margin-top: 0px;
	position:relative;
	z-index:9;
}
.sidebar_banner{
	@media(max-width:991px){
		display:none;
	}
}
.bg_collection {
	margin: 0px 0 30px;
	@media (max-width: 767px) {
		margin:0px;
	}
}
.main_container{
	@media(min-width:992px){
		order:2;
	}
	.border-bottom{
		border-bottom:1px solid #ebebeb;
		height:1px;
		margin-bottom: 30px;
		@media(max-width:767px){
			border: none !important;
			height: unset;
			margin-bottom: 0;
		}
	}
}
.left-content{
	@media(min-width:992px){
		order:1;
	}
}
.category-products, .cls_search {
	.item_product_main .product-item-main {
		border: solid 1px #ebebeb;
	}
	.item_product_main .product-item-main .product-bottom .blockprice{
		padding-bottom:60px;
	}
	.item_product_main .product-item-main .product-bottom .form-nut-grid .group_action {
		@media (min-width:992px) and (max-width: 1199px) {
			left:-15px;
		}
	}
}
.wrap_background_aside {
	width: 100%;
	float: left;
	&.asidecollection {
		@media (max-width:1199px) {
			margin-top:0px;
		}
	}
}
.aside-mini-list-product{
	.aside-content{
		border:0;
		.slicklist{
			.slick-arrow{
				font-size: 0;
				line-height: 0;
				position: absolute;
				top: -40px;
				display: block;
				width: 25px;
				height: 25px;
				padding: 0;
				-webkit-transform: translate(0, -50%);
				-ms-transform: translate(0, -50%);
				transform: translate(0, -50%);
				cursor: pointer;
				color: transparent;
				border: none;
				outline: none;
				background: transparent;
				line-height: 25px;
				text-align: center;
				z-index: 2;
				@media(max-width: 767px){
					display:none !important;
				}
				&.slick-prev {
					left: auto;
					right:28px;
					&:before{
						content: "\f0d9";
						font-family:"Font Awesome\ 5 Free";
						background:#e1e1e1;
						font-weight:900;
						height: 25px;
						width: 25px;
						font-size:14px;
						color:#fff;
						display: block;
						-webkit-transition: all ease 0.3s;
						-moz-transition: all ease 0.3s;
						-ms-transition: all ease 0.3s;
						-o-transition: all ease 0.3s;
						transition: all ease 0.3s;
					}
					&:hover{
						&:before{
							background:$hover;
						}
					}
				}
				&.slick-next {
					right: 0;
					&:before{
						content: "\f0da";
						font-family:"Font Awesome\ 5 Free";
						background:#e1e1e1;
						font-weight:900;
						color:#fff;
						font-size:14px;
						height: 25px;
						width: 25px;
						display: block;
						-webkit-transition: all ease 0.3s;
						-moz-transition: all ease 0.3s;
						-ms-transition: all ease 0.3s;
						-o-transition: all ease 0.3s;
						transition: all ease 0.3s;
					}
					&:hover{
						&:before{
							background:$hover;
						}
					}
				}
			}
		}
	}
}
.aside-mini-list-product{
	margin:0;
}
.item_product_main_list {
	width:100%;
	float:left;
	.product_main_list_mini{
		width:100%;
		float:left;
		margin-bottom: 15px;
		padding-bottom: 15px;
		form {
			margin-bottom:0;
			.product-thumbnail {
				width:80px;
				float:left;
				border: #e1e1e1 1px solid ;
				padding:1px;
				-webkit-transition: all ease 0.3s;
				-moz-transition: all ease 0.3s;
				-ms-transition: all ease 0.3s;
				-o-transition: all ease 0.3s;
				transition: all ease 0.3s;
				@media(max-width:992px) and (min-width:768px){
					width:160px;
				}
				@media(max-width:767px){
					width:100px;
				}
				&:hover{
					border:1px solid $main-color;
				}
			}
			.product-info {
				width:calc(100% - 80px);
				-webkit-width:calc(100% - 80px);
				-moz-width:calc(100% - 80px);
				-o-width:calc(100% - 80px);
				-os-width:calc(100% - 80px);
				padding-left:20px;
				float:left;
				@media(max-width:992px) and (min-width:768px){
					width:calc(100% - 160px);
				}
				@media(max-width:767px){
					width:calc(100% - 100px);
				}
				.product-name {
					font-size: 14px;
					margin: 0 0 5px;
				}
				.price-box {
					width:100%;
					float:left;
					text-align: left;
					font-size:13px;
					font-weight:700;
					margin-bottom:15px;
					color:$price;
					font-family: Roboto, Arial, Helvetica, sans-serif;
					.compare-price {
						color: #777777;
						text-decoration: line-through;
						font-size: 13px;
						font-weight:400;
					}
				}
				.btn-views {
					height:35px;
					line-height:35px;
					background: #fff;
					border: 1px solid $main-color;
					padding:0 10px;
					box-shadow:none;
					font-family: 'Open Sans', sans-serif;
					font-size: 11px;
					font-weight: 700;
					margin-left: 0;
					color:$main-color;
					@media (max-width:1199px) and (min-width:992px) {
						padding:0 5px;
						font-size:8px;
					}
					.fa {
						padding-right:5px;
						font-size:11px;
					}
					@media (max-width:767px) {
						font-size:10px;
						.fa {
							font-size:10px;
						}
					}
					&:hover {
						background:$hover;
						border-color:$hover;
						color:#fff;
					}
				}
			}
		}
	}
}
.slider-items-products {
	position:relative;
