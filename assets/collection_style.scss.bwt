	}
}


#sort-by {
	.button-asc, .button-desc {
		background-color: #f8f8f8;
		box-shadow: none;
		float: right;
		height: 30px;
		line-height: 28px;
		margin-left: 5px;
		text-align: center;
		width: 30px;
	}
	float: right;
	font-size: 12px;
	text-align: left;
	label {
		float: left;
		font-weight: 400;
		line-height: 33px;
		font-size: 13px;
		margin-right: 6px;
		margin-bottom: 0;
		color: $text-color;
		@media(max-width:767px){
			display:none !important;
		}
	}
	ul {
		margin: 0;
		padding: 0;
		list-style: none;
		display: inline;
		float: right;
	}
	a {
		&:link, &:visited {
			color: #333;
			text-decoration: none;
		}
	}
	> ul > li {
		-webkit-appearance: none;
		-moz-appearance: none;
		appearance: none;
		background-position: right center;
		background-repeat: no-repeat;
		background-position: right 10px center;
		padding-right: 28px;
		text-indent: .01px;
		cursor: pointer;
		line-height: 34px;
		background-image: url({{'ico-select.png'|asset_url}})!important;
		border-radius: 3px;
		&:hover{
			border-bottom-right-radius: 0;
			border-bottom-left-radius: 0;
		}
	}
	li {
		background: none repeat scroll 0 0 #fff;
		cursor: pointer;
		margin: 0 auto;
		outline: medium none;
		position: relative;
		width: 180px;
		padding: 0 20px;
		float: left;
		color: $text-color;
		font-weight:400;
		border: 0;
		font-family:$font-base;
		border: 1px solid #e5e5e5;
		font-size: 14px;
		> a {
			display: inline-block;
			width: 100%;
			line-height: 36px;
			transition: all 0s ease-in-out;
			&:hover{
				color:$hover;
			}
		}
		li {
			width: 100%;
			margin-top: 0px;
			border-top: none;
			border-right: none;
			border-left: none;
			padding:0 20px;
			a:hover {
				color: $hover;
			}
		}
		&:hover {
			background-color: #fff;
		}
		li:hover {
			background-color: #fff;
			color: #007fb8;
		}
	}
	ul ul {
		position: absolute;
		visibility: hidden;
		width: 180px;
		left: -1px;
		top: 34px;
		background: none repeat scroll 0 0 #F8F8F8;
		z-index: 10000;
		border: 1px solid #ebebeb;
		border-radius: 3px;
		overflow: hidden;
		border-top-right-radius: 0;
		border-top-left-radius: 0;
		overflow: hidden;
		background: #fff;
		padding-left: 0px !important;
		li {
			&:last-child {
				border-bottom: none;
			}
		}
		ul {
			position: absolute;
			left: 100%;
			top: -2px;
			border: solid 1px transparent;
		}
	}
	li:hover > ul {
		visibility: visible;
	}
}

